<!--pages/register/register.wxml-->
<view id="total">
  <form bindsubmit="create_login">
    <view id="t1" style="position: relative; left: 0rpx; top: 195rpx">
      <text>账号</text>
      <input bind:change='inputName' type="text" name="username" id="use" placeholder="输入您的账号" value="{{name}}" style="position: relative; left: 12rpx; top: -25rpx"></input>
    </view>
    <view id="t2" style="position: relative; left: 0rpx; top: 225rpx">
      <text>密码</text>
      <input bind:change='inputPassword' type="password" name="password" id="pass1" placeholder="输入您的密码"
        value="{{password}}"></input>
    </view>
    <view id="t2" style="position: relative; left: 0rpx; top: 263rpx">
      <text>密码</text>
      <input bind:change='inputConfirmPassword' type="password" name="password" id="pass1" placeholder="确认您的密码"
        value="{{password}}"></input>
    </view>
    <button bindtap="goto_register_newuser" id="btn1" form-type="submit" style="position: relative; left: 0rpx; top: 319rpx"><text>注册</text></button>

    <text style="position: relative; left: 338rpx; top: -357rpx">注册</text>
  </form>

 




  
  </view>
